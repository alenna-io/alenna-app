{
  "buildCommand": "pnpm install --frozen-lockfile && pnpm build",
  "outputDirectory": "dist",
  "framework": "vite",
  "headers": [
    {
      "source": "/(.*\\.(svg|ico|png|jpg|jpeg|gif|webp|woff|woff2|ttf|otf|eot|css|js))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.(svg|ico|png|jpg|jpeg|gif|webp|woff|woff2|ttf|otf|eot|css|js)$).*)",
      "destination": "/index.html"
    }
  ]
}


